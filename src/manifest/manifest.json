{
  "name": "齐天-业主版",
  "version": "3.0",
  "manifest_version": 2,
  "description": "齐天-业主版",
  "icons": {
    "16": "static/logoS.png",
    "48": "static/logoS.png",
    "128": "static/logoS.png"
  },
  "browser_action": {
    "default_icon": "static/logoS.png",
    "default_title": "齐天-业主版",
    "default_popup": "popup.html"
  },
  "homepage_url": "https://www.oyohotels.cn/",
  "content_scripts": [{
      "js": [
        "meituan.js"
      ],
      "matches": [
        "*://eb.meituan.com/*",
        "*://ebooking.meituan.com/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "ctrip.js"

      ],
      "matches": [
        "*://ebooking.ctrip.com/*"
      ],
      "run_at": "document_idle"
    },
    {
      "js": [
        "fliggy.js"
      ],
      "matches": [
        "*://ebooking.fliggy.com/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "storage",
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "tabs",
    "activeTab",
    "proxy",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}